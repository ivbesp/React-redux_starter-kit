{
  "name": "React-Redux_starter-kit ",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "-----MAIN-SCRIPTS": "-----",
    "start": "npm run build && npm run watch & npm run server",
    "deploy": "gh-pages -d public",
    "production": "npm run lint:js && npm run lint:css && npm run build:scss:min && npm run autoprefixer && npm run build:js && npm run prettier",
    "-----SERVER": "-----",
    "server": "browser-sync start --server --files 'public/**/*.(js|css|html)' --ss 'public' --reload-debounce 500",
    "-----BUILDS": "-----",
    "build:scss": "node-sass src/scss/style.scss -o public/css/",
    "build:js": "browserify src/app/index.js -o public/js/bundle.js -t [ babelify --presets [ react es2015 ] ]",
    "build": "concurrently 'npm run build:scss' 'npm run build:js'",
    "build:scss:min": "node-sass --output-style compressed src/scss/style.scss -o public/css/",
    "-----WATCHERS": "-----",
    "watch:css": "npm run build:scss && npm run build:scss -- -w",
    "watch:js": "watchify src/app/index.js -o public/js/bundle.js -t [ babelify --presets [ react es2015 ] ]",
    "watch": "concurrently \"npm run watch:js\" \"npm run watch:css\" ",
    "-----FILESYSTEM": "-----",
    "clear:git": "rimraf .git",
    "-----OPTIMIZATION": "-----",
    "prettier": "prettier 'public/**/*.?(js|jsx)' --write",
    "autoprefixer": "postcss -u autoprefixer -r public/css/*",
    "-----LINTS": "-----",
    "lint:js": "eslint public/js/ || true",
    "lint:css": "csslint public/css/style.css || true"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "autoprefixer": "^8.1.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babelify": "^7.3.0",
    "browser-sync": "^2.23.6",
    "browserify": "^16.1.0",
    "concurrently": "^3.4.0",
    "csslint": "^1.0.5",
    "eslint": "^4.18.2",
    "gh-pages": "^1.1.0",
    "imagemin": "^5.3.1",
    "node-sass": "^4.7.2",
    "postcss-cli": "^5.0.0",
    "prettier": "^1.11.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "redux": "^3.7.2",
    "rimraf": "^2.6.2",
    "watchify": "^3.9.0"
  },
  "devDependencies": {}
}
